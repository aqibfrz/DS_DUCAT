   "source": [
    "def fact(n):\n",
    "    if n==0 or n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        \n",
    "        return n * fact(n-1)\n",
    "    \n",
    "n=4\n",
    "print(fact(n))"
   ]
